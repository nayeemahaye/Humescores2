<wpml-config>
    <custom-fields>
        <custom-field action="copy">page-bg-color</custom-field>
        <custom-field action="copy">page-bg-opacity</custom-field>
        <custom-field action="copy">page-txt-color</custom-field>
        <custom-field action="copy">hero-enabled</custom-field>
        <custom-field action="copy">hero-color</custom-field>
        <custom-field action="translate">hero-image</custom-field>
        <custom-field action="translate">hero-video</custom-field>
        <custom-field action="copy">hero-video-loop</custom-field>
        <custom-field action="translate">page-logo</custom-field>
        <custom-field action="copy">page-scheme</custom-field>
        <custom-field action="copy">page-acc-color</custom-field>
        <custom-field action="copy">hero-gallery</custom-field>
        <custom-field action="copy">hero-hide-text</custom-field>
        <custom-field action="copy">hero-vertical-text</custom-field>
        <custom-field action="translate">hero-tagline</custom-field>
        <custom-field action="copy">project-size</custom-field>
        <custom-field action="translate">portfolio-unique</custom-field>
        <custom-field action="translate">portfolio-lightbox</custom-field>
        <custom-field action="copy">hero-gallery-autoplay</custom-field>
        <custom-field action="copy">portfolio-lightbox-type</custom-field>
        <custom-field action="translate">portfolio-lightbox-url</custom-field>
        <custom-field action="copy">hreo-hide-title</custom-field>
        <custom-field action="translate">portfolio_featured-thumbnail-secondary_thumbnail_id</custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1">portfolio</custom-type>
        <custom-type translate="1">portfolio_category</custom-type>
        <custom-type translate="1">portfolio_tag</custom-type>
        <custom-type translate="1">product</custom-type>
        <custom-type translate="1">product_category</custom-type>
        <custom-type translate="1">product_tag<custom-type>
    </custom-types>       
    <admin-texts>
        <key name="calafate_logo" />
        <key name="calafate_logo_blog" />
        <key name="calafate_logo_woocommerce" />
        <key name="calafate_portfolio_page" />
        <key name="calafate_blog_carousel" />
    </admin-texts>    
    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>    
        <key name="icl_language_switcher_sidebar">0</key>  
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">0</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>    
    </language-switcher-settings>    
</wpml-config>